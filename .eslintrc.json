{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": ["error", "never"],
    "jsx-quotes": [2, "prefer-single"],
    "no-param-reassign": ["error", { "props": false }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-one-expression-per-line": [0, { "allow": "literal"}],
    "camelcase": "off"
  },
  "env": {
    "browser": true
  }
}